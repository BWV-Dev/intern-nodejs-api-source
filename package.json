{
  "name": "intern-nodejs-api-source",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "clean": "rimraf ./dist",
    "build": "yarn clean && tsc",
    "test": "yarn run check",
    "migrate:up": "yarn clean && yarn build && sequelize db:migrate",
    "migrate:down": "sequelize db:migrate:undo",
    "migrate:undo-all": "sequelize db:migrate:undo:all",
    "lint": "eslint --ext .ts \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "check": "yarn lint && yarn prettier"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:BWV-Dev/intern-nodejs-api-source.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/qs": "^6.9.14",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "command-line-args": "^5.2.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mysql2": "^3.9.3",
    "qs": "^6.12.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.2",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/command-line-args": "^5.2.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "typescript": "4.5.5"
  }
}
